package base

import ()

type Portfolio struct {
	SecurityEntrusts []Entrust
	FutureEntrusts   []Entrust
}

type Entrust struct {
	Code          string  // 证券代码
	Price         float64 // 委托价格
	Vol           int     // 委托数量
	OpenCloseFlag int     // 开平标记 1：开，2：平
	BS            int     // 买卖方向 1：买，2：卖
	TimeStamp     int     // 订单生成时间戳
}
