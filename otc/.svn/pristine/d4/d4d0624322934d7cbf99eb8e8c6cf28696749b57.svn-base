create table dptrade(
	BrokerID	varchar(32),
	InvestorID	varchar(32),
	InstrumentID	varchar(32),
	OrderRef	varchar(32),
	UserID	varchar(32),
	ExchangeID	varchar(32),
	TradeID	varchar(32),
	Direction	varchar(32),
	OrderSysID	varchar(32),
	ParticipantID	varchar(32),
	ClientID	varchar(32),
	TradingRole	varchar(32),
	ExchangeInstID	varchar(32),
	OffsetFlag	varchar(32),
	HedgeFlag	varchar(32),
	Price	double,
	Volume	int,
	TradeDate	varchar(32),
	TradeTime	varchar(32),
	TradeType	varchar(32),
	PriceSource	varchar(32),
	TraderID	varchar(32),
	OrderLocalID	varchar(32),
	ClearingPartID	varchar(32),
	BusinessUnit	varchar(32),
	SequenceNo	int,
	TradingDay	varchar(32),
	SettlementID	int,
	BrokerOrderSeq	int,
	TradeSource	varchar(32),
	timestamp timestamp null default current_timestamp on update current_timestamp)