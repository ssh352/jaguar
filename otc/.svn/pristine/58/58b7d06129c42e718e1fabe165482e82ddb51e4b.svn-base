[t2sdk]
;许可证文件
license_file=license.dat 
;服务器地址
servers=127.0.0.1:9004
;接受缓存初始化大小
init_recv_buf_size=10240
;发送缓存初始化大小
init_send_buf_size=10240
;发送队列
send_queue_size=100000
[safe]
;安全级别
safe_level=none
;客户号
client_id=123456
;客户密码
comm_pwd=888888
;ssl证书
cert_file=xxx.pem
;ssl证书密码
cert_pwd=xxxxxxxx
[mc]
;如果需要消息中心的订阅发布功能，这个配置项必须配置
client_name=xuxp
[publish]
;主题名字
topic_name=secu.rzrq_customer_share
;过滤字段的个数，指明下面的过滤有几个,和主题定义的过滤字段相关，主题定义的过滤字段，这里都必须有
filter_count=2
;过滤字段1名字，一共可以设置六个
filter_name1=branch_no
;过滤字段1值，一共可以设置六个
filter_value1=A
filter_name2=fund_account
filter_value2=600570

